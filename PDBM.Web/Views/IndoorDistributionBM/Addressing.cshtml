    function onAddressingClick() {
        var rows = dgAddressing.getSelecteds();
        if (rows.length == 0) {
            mini.alert("请选择一条要寻址确认的数据");
        }
        else {
            ClearFrmAddressing();
            winAddressing.setTitle("寻址确认");
            winAddressing.show();
            var el = winAddressing.getEl();
            mini.mask({
                el: el,
                cls: 'mini-mask-loading',
                html: '加载中...'
            });
            setTimeout(function () {
                $.ajax({
                    url: "@Url.RouteUrl(new { controller = "IndoorDistributionBM", action = "GetAddressingById" })",
                    data: { Id: rows[0].Id, PlanningId: rows[0].PlanningId },
                    type: "get",
                    async: false,
                    success: function (result, textStatus) {
                        if (result.Id != null) {
                            cb_ReseauId.setUrl("@Url.RouteUrl(new { controller = "BaseData", action = "GetUsedReseaus" })" + "/" + result.AreaId + "?getType=1");
                            frmAddressing.setData(result);
                            //beAreaManagerId.setText(result.AreaManagerText);
                            //beDesignCustomerId.setText(result.DesignCustomerText);
                            txtPlaceName.focus();
                        }
                        else {
                            winAddressing.hide();
                        }
                    },
                    error: function (xmlHttpRequest, textStatus, errorThrown) {
                        winAddressing.hide();
                    }
                });
            }, 50);
            setTimeout(function () {
                mini.unmask(el);
            }, 300);
        }
    }

    function onMapEditClick() {
        var el = winAddressing.getEl();
        mini.mask({
            el: el,
