                            ShowSucessMessage(result.Message);
                            BindDgOperatorsSharingCollection();
                        }
                    },
                    error: function (xmlHttpRequest, textStatus, errorThrown) {
                        mini.hideMessageBox(loading);
                    }
                });
            }
        }
    }

