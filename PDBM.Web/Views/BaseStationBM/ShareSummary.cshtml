            mini.alert("请先保存铁塔公司基站改造");
        }
        else {
            var rows = dgOperatorsSharingCollection.findRows(function (row) {
                if (dgOperatorsSharingCollection.isSelected(row) == true && row.RemodelingId == "00000000-0000-0000-0000-000000000000") {
                    return true;
                }
                else if (dgOperatorsSharingCollection.isSelected(row) == false && row.RemodelingId != "00000000-0000-0000-0000-000000000000") {
                    return true;
                }
            });
            if (rows.length == 0) {
                mini.alert("未修改数据,无需提交关联");
            }
            else {
                var loading = mini.loading("处理中,请稍后...", "关联数据");
