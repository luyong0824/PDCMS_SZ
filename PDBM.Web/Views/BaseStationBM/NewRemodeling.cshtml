<script type="text/javascript">
    mini.parse();

    var btnSearch = mini.get("btnSearch");
    var dgRemodeling = mini.get("dgRemodeling");
    var winQuery = mini.get("winQuery");
    var dpBeginDate = mini.get("dpBeginDate");
    var dpEndDate = mini.get("dpEndDate");
    var txtPlaceCode = mini.get("txtPlaceCode");
    var txtPlaceName = mini.get("txtPlaceName");
    var cbAreaId = mini.get("cbAreaId");
    var cbReseauId = mini.get("cbReseauId");
    var cbPlaceCategoryId = mini.get("cbPlaceCategoryId");
    var cbUrgency = mini.get("cbUrgency");
    var cbOrderState = mini.get("cbOrderState");
    var beCreateUserId = mini.get("beCreateUserId");
    var winRemodeling = mini.get("winRemodeling");
    var frmRemodeling = new mini.Form("#frmRemodeling");
    var txtRemodelingId = mini.get("Id");
    var txtMobileDemand = mini.get("MobileDemand");
    var txtTelecomDemand = mini.get("TelecomDemand");
    var txtUnicomDemand = mini.get("UnicomDemand");
    var dpCreateDate = mini.get("CreateDate");
    var txt_PlaceCode = mini.get("PlaceCode");
    var bePlaceId = mini.get("PlaceId");
    var cb_AreaId = mini.get("AreaId");
    var cb_PlaceCategoryId = mini.get("PlaceCategoryId");
    var cb_ReseauId = mini.get("ReseauId");
    var txtLng = mini.get("Lng");
    var txtLat = mini.get("Lat");
    var cbPropertyRight = mini.get("PropertyRight");
    var cbMobileShare = mini.get("MobileShare");
    var cbTelecomShare = mini.get("TelecomShare");
    var cbUnicomShare = mini.get("UnicomShare");
    var cb_OrderState = mini.get("OrderState");
    var ck_Mobile = mini.get("ck_Mobile");
    var txt_MobilePoleNumber = mini.get("MobilePoleNumber");
    var txt_MobileCabinetNumber = mini.get("MobileCabinetNumber");
    var txt_MobilePowerUsed = mini.get("MobilePowerUsed");
    var beMobileUserId = mini.get("MobileUserId");
    var ck_Telecom = mini.get("ck_Telecom");
    var txt_TelecomPoleNumber = mini.get("TelecomPoleNumber");
    var txt_TelecomCabinetNumber = mini.get("TelecomCabinetNumber");
    var txt_TelecomPowerUsed = mini.get("TelecomPowerUsed");
    var beTelecomUserId = mini.get("TelecomUserId");
    var ck_Unicom = mini.get("ck_Unicom");
    var txt_UnicomPoleNumber = mini.get("UnicomPoleNumber");
    var txt_UnicomCabinetNumber = mini.get("UnicomCabinetNumber");
    var txt_UnicomPowerUsed = mini.get("UnicomPowerUsed");
    var beUnicomUserId = mini.get("UnicomUserId");
    var winImport = mini.get("winImport");
    var beFile = mini.get("beFile");
    var winImportError = mini.get("winImportError");
    var dgImportError = mini.get("dgImportError");

    function SetEnabledMobile(checked) {
        txt_MobilePoleNumber.setEnabled(checked);
        txt_MobileCabinetNumber.setEnabled(checked);
        txt_MobilePowerUsed.setEnabled(checked);
        beMobileUserId.setEnabled(checked);

        if (checked == false) {
            txt_MobilePoleNumber.setValue(0);
            txt_MobileCabinetNumber.setValue(0);
            txt_MobilePowerUsed.setValue(0);
            beMobileUserId.setValue("00000000-0000-0000-0000-000000000000");
            beMobileUserId.setText("请选择");
        }
    }

    function SetEnabledTelecom(checked) {
        txt_TelecomPoleNumber.setEnabled(checked);
        txt_TelecomCabinetNumber.setEnabled(checked);
        txt_TelecomPowerUsed.setEnabled(checked);
        beTelecomUserId.setEnabled(checked);

        if (checked == false) {
            txt_TelecomPoleNumber.setValue(0);
            txt_TelecomCabinetNumber.setValue(0);
            txt_TelecomPowerUsed.setValue(0);
            beTelecomUserId.setValue("00000000-0000-0000-0000-000000000000");
            beTelecomUserId.setText("请选择");
        }
    }

    function SetEnabledUnicom(checked) {
        txt_UnicomPoleNumber.setEnabled(checked);
        txt_UnicomCabinetNumber.setEnabled(checked);
        txt_UnicomPowerUsed.setEnabled(checked);
        beUnicomUserId.setEnabled(checked);

        if (checked == false) {
            txt_UnicomPoleNumber.setValue(0);
            txt_UnicomCabinetNumber.setValue(0);
            txt_UnicomPowerUsed.setValue(0);
            beUnicomUserId.setValue("00000000-0000-0000-0000-000000000000");
            beUnicomUserId.setText("请选择");
        }
    }

    function onValueChangedMobile(e) {
        var checked = this.getChecked();
        SetEnabledMobile(checked);
        if (checked == true) {
            txtMobileDemand.setValue("1");
        }
        else {
            txtMobileDemand.setValue("2");
        }
    }

    function onValueChangedTelecom(e) {
        var checked = this.getChecked();
        SetEnabledTelecom(checked);
