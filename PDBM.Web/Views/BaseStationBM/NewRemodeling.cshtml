    function onEditClick() {
        var rows = dgRemodeling.getSelecteds();
        if (rows.length == 0) {
            mini.alert("请选择一条要修改的数据");
        }
        else {
            ClearFrmRemodeling();
            winRemodeling.setTitle("修改基站改造");
            winRemodeling.show();
            var el = winRemodeling.getEl();
            mini.mask({
                el: el,
                cls: 'mini-mask-loading',
                html: '加载中...'
            });
            setTimeout(function () {
                $.ajax({
                    url: "@Url.RouteUrl(new { controller = "BaseStationBM", action = "GetNewRemodelingById" })" + "/" + rows[0].Id,
                    type: "get",
                    async: false,
                    success: function (result, textStatus) {
                        if (result.Id != null && result.Id == rows[0].Id) {
                            BindReseauId(result.AreaId);
                            frmRemodeling.setData(result);
                            dpCreateDate.setValue(result.CreateDateText);
                            bePlaceId.setText(result.PlaceName);
                            if (result.MobileDemand == 1) {
                                ck_Mobile.setValue(true);
                                SetEnabledMobile(true);
                            }
                            else {
                                ck_Mobile.setValue(false);
                                SetEnabledMobile(false);
                            }
                            beMobileUserId.setValue(result.MobileUserId);
                            beMobileUserId.setText(result.MobileUserFullName);
                            if (result.TelecomDemand == 1) {
                                ck_Telecom.setValue(true);
                                SetEnabledTelecom(true);
                            }
                            else {
                                ck_Telecom.setValue(false);
                                SetEnabledTelecom(false);
                            }
                            beTelecomUserId.setValue(result.TelecomUserId);
                            beTelecomUserId.setText(result.TelecomUserFullName);
                            if (result.UnicomDemand == 1) {
                                ck_Unicom.setValue(true);
                                SetEnabledUnicom(true);
                            }
                            else {
                                ck_Unicom.setValue(false);
                                SetEnabledUnicom(false);
                            }
                            beUnicomUserId.setValue(result.UnicomUserId);
                            beUnicomUserId.setText(result.UnicomUserFullName);
                        }
                        else {
                            winRemodeling.hide();
                        }
                    },
                    error: function (xmlHttpRequest, textStatus, errorThrown) {
                        winRemodeling.hide();
                    }
                });
            }, 50);
            setTimeout(function () {
                mini.unmask(el);
            }, 300);
        }
    }

