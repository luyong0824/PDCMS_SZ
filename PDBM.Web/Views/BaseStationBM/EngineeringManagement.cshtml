    function Init() {
        dgEngineeringManagement.hideColumn(2);
        dgEngineeringManagement.hideColumn(3);
        dgEngineeringManagement.hideColumn(4);
        BindCbPlaceCategoryId();
        BindDgEngineeringManagement();
