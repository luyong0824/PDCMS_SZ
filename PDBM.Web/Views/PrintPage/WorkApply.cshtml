@{
    Layout = null;
}
<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>隐患上报</title>
    @Styles.Render("~/Content/print_css")
</head>
<body>
    <div style="width:100%; text-align:center;">
        <table class="table" cellpadding="0" cellspacing="0" style="margin: auto; width: 60%">
            <tr>
                <td colspan="4" align="center" style="font-size:11pt;">
                    <b>
                        @if (@ViewData["OrderCode"].ToString() == "")
                        {
                            @:隐患上报单
                        }
                        else
                        {
                            @:隐患上报单
                        }
                    </b>
                </td>
            </tr>
            <tr>
                <td style="width:90px;" align="right">申请部门</td>
                <td style="width:200px;" align="left">@ViewData["DepartmentName"]</td>
                <td style="width:90px;" align="right">申请人</td>
                <td style="width:200px;" align="left">@ViewData["CreateFullName"]</td>
            </tr>
            <tr>
                <td style="width:90px;" align="right">派单人</td>
                <td style="width:200px;" align="left">@ViewData["SendFullName"]</td>
                <td style="width:90px;" align="right">申请日期</td>
                <td style="width:200px;" align="left">@ViewData["CreateDate"]</td>
            </tr>
            <tr>
                <td align="right">标题</td>
                <td align="left" colspan="3">@ViewData["Title"]</td>
            </tr>
            <tr>
                <td align="right">申请事由</td>
                <td align="left" colspan="3">@ViewData["ApplyReason"]</td>
            </tr>
            <tr>
                <td style="width:90px;" align="right">网格</td>
                <td style="width:200px;" align="left">@ViewData["ReseauName"]</td>
                <td style="width:90px;" align="right">申请单位</td>
                <td style="width:200px;" align="left">@ViewData["CustomerName"]</td>
            </tr>
        </table>

        @*<table class="table" cellpadding="0" cellspacing="0" style="margin: auto; width: 60%">
            @if (@ViewData["WFActivityInstancesInfoHtml"].ToString() != "")
            {
                <tr>
                    <td colspan="4" style="width:1000px;">
                        @Html.Raw(ViewData["WFActivityInstancesInfoHtml"])
                    </td>
                </tr>
            }
        </table>*@
    </div>
</body>
</html>
