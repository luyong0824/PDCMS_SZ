@{
    Layout = null;
}
<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>苏州铁塔公司零星用工派工单</title>
    @Styles.Render("~/Content/print_css")
</head>
<body>
    <div style="width:100%; text-align:center;">
        <table style="width:100%">
            <tr>
                <td colspan="4" align="center" style="font-size:11pt;">
                    <b>
                        @if (@ViewData["OrderCode"].ToString() == "")
                        {
                            @:苏州铁塔公司零星用工派工单
                        }
                        else
                        {
                            @:苏州铁塔公司零星用工派工单(@ViewData["OrderCode"])
                        }
                    </b>
                </td>
            </tr>
        </table>
        <fieldset>
            <legend><b>派工单</b></legend>
            <table class="table" cellpadding="0" cellspacing="0" style="margin: auto; width: 1000px">
                <tr>
                    <td style="width:100px;" align="right">派工人</td>
                    <td style="width:300px;" align="left">@ViewData["FullName"]</td>
                    <td style="width:100px;" align="right">派工日期</td>
                    <td style="width:300px;" align="left">@ViewData["CreateDate"]</td>
                </tr>
                <tr>
                    <td align="right">标题</td>
                    <td align="left">@ViewData["Title"]</td>
                    <td align="right">网格</td>
                    <td align="left">@ViewData["ReseauName"]</td>
                </tr>
                <tr>
                    <td align="right">工单类型</td>
                    <td align="left">@ViewData["ClassName"]</td>
                    <td align="right">要求派工日期</td>
                    <td align="left">@ViewData["RequireSendDate"]</td>
                </tr>
                <tr>
                    <td align="right">现场联系人</td>
                    <td align="left">@ViewData["SceneContactMan"]</td>
                    <td align="right">现场联系电话</td>
                    <td align="left">@ViewData["SceneContactTel"]</td>
                </tr>
                <tr>
                    <td align="right">派工单位</td>
                    <td align="left">@ViewData["CustomerName"]</td>
                    <td align="right">派工时长</td>
                    <td align="left">@ViewData["Days"]</td>
                </tr>
                <tr>
                    <td align="right">派工联系人</td>
                    <td align="left">@ViewData["MaintainContactMan"]</td>
                    <td align="right">派工联系电话</td>
                    <td align="left">@ViewData["MainTainContactTel"]</td>
                </tr>
                <tr>
                    <td align="right">工作内容</td>
                    <td align="left">@ViewData["WorkContent"]</td>
                    <td align="right">用人要求</td>
                    <td align="left">@ViewData["HumanRequire"]</td>
                </tr>
                <tr>
                    <td align="right">用车要求</td>
                    <td align="left">@ViewData["CarRequire"]</td>
                    <td align="right">材料要求</td>
                    <td align="left">@ViewData["MaterialRequire"]</td>
                </tr>
            </table>
        </fieldset>
        <fieldset>
            <legend><b>结算单</b></legend>
            <table class="table" cellpadding="0" cellspacing="0" style="margin: auto; width: 1000px">
                <tr>
                    <td style="width:100px;" align="right">工作起始时间</td>
                    <td style="width:300px;" align="left">@ViewData["WorkBeginDate"]</td>
                    <td style="width:100px;" align="right">工作结束时间</td>
                    <td style="width:300px;" align="left">@ViewData["WorkEndDate"]</td>
                </tr>
                <tr>
                    <td align="right">执行情况</td>
                    <td align="left">@ViewData["ExecuteSituation"]</td>
                    <td align="right">材料消耗</td>
                    <td align="left">@ViewData["MaterialConsumption"]</td>
                </tr>
                <tr>
                    <td align="right">人员数量</td>
                    <td align="left">@ViewData["PersonnelNumber"]</td>
                    <td align="right">车辆类型</td>
                    <td align="left">@ViewData["CarType"]</td>
                </tr>
                <tr>
                    <td align="right">是否完成</td>
                    <td align="left">@ViewData["IsFinish"]</td>
                </tr>
                <tr>
                    <td align="right">结算登记人</td>
                    <td align="left">@ViewData["RegisterFullName"]</td>
                    <td align="right">结算登记日期</td>
                    <td align="left">@ViewData["RegisterDate"]</td>
                </tr>
            </table>
        </fieldset>
        <fieldset>
            <legend><b>各天执行情况</b></legend>
            <table class="table" cellpadding="0" cellspacing="0" style="margin: auto; width: 1000px">
                @if (@ViewData["WorkOrderDetailInfoHtml"].ToString() != "")
                {
                    <tr>
                        <td colspan="4" style="width:1000px;">
                            @Html.Raw(ViewData["WorkOrderDetailInfoHtml"])
                        </td>
                    </tr>
                }
            </table>
        </fieldset>
        <fieldset>
            <legend><b>审批信息</b></legend>
            <table class="table" cellpadding="0" cellspacing="0" style="margin: auto; width: 1000px">
                @if (@ViewData["WFActivityInstancesInfoHtml"].ToString() != "")
                {
                    <tr>
                        <td colspan="8" style="width:1000px;">
                            @Html.Raw(ViewData["WFActivityInstancesInfoHtml"])
                        </td>
                    </tr>
                }
            </table>
        </fieldset>
        <br />
        <br />
        <br />
        <br />
        <br />
        <table style="width:100%">
            <tr>
                <td align="right" style="font-size: 11pt; width: 19%">
                    <b>
                        工程经理签字
                    </b>
                </td>
                <td colspan="3" align="left">
                    <u>___________________________</u>
                </td>
            </tr>
        </table>
        <br />
        <br />
        <br />
        <br />
        <br />
        <table style="width:100%">
            <tr>
                <td align="right" style="font-size: 11pt; width: 19%; ">
                    <b>
                        主管签字
                    </b>
                </td>
                <td colspan="3" align="left">
                    <u>___________________________</u>
                </td>
            </tr>
        </table>
        <br />
        <br />
        <br />
        <br />
        <br />
        <table style="width:100%">
            <tr>
                <td align="right" style="font-size: 11pt; width: 19%; ">
                    <b>
                        经理签字
                    </b>
                </td>
                <td colspan="3" align="left">
                    <u>___________________________</u>
                </td>
            </tr>
        </table>
        <br />
        <br />
        <br />
        <br />
        <br />
        <table style="width:100%">
            <tr>
                <td align="right" style="font-size: 11pt; width: 19%">
                    <b>
                        审计签字
                    </b>
                </td>
                <td colspan="3" align="left">
                    <u>___________________________</u>
                </td>
            </tr>
        </table>
    </div>
</body>
</html>
